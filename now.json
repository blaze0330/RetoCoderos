{
  "version": 2,
  "name": "RetoCoderosJunio2019",
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": { "distDir": "public" }
    }
  ],
  "routes": [
    { "src": "/bundle.css", "dest": "/bundle.css" },
    { "src": "/bundle.js", "dest": "/bundle.js" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
